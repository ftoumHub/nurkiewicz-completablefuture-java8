<?xml version="1.0" encoding="UTF-8" ?>
<configuration>

	<logger name="com" level="INFO"/>
	<logger name="com.reactive" level="ALL"/>

	<root level="ALL">
		<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
			<encoder>
				<pattern>%5r | %10thread | %m%n%rEx</pattern>
			</encoder>
		</appender>
	</root>
</configuration>
